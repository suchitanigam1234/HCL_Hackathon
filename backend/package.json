{
  "name": "healthcare-wellness-backend",
  "version": "1.0.0",
  "description": "Backend API for Healthcare Wellness & Preventive Care Portal",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "init": "node scripts/init.js",
    "seed:rules": "node scripts/seedRules.js",
    "seed:data": "node scripts/seedData.js",
    "postinstall": "echo 'Run npm run init to seed database on first setup'"
  },
  "keywords": [
    "healthcare",
    "wellness",
    "preventive-care",
    "express",
    "mongodb"
  ],
  "author": "prakhar2909",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "express-validator": "^7.0.1",
    "cron": "^3.1.6",
    "qrcode": "^1.5.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
    "testMatch": ["**/tests/**/*.test.js"],
    "maxWorkers": 1,
    "runInBand": true,
    "collectCoverageFrom": [
      "controllers/**/*.js",
      "models/**/*.js",
      "routes/**/*.js",
      "middleware/**/*.js",
      "!**/node_modules/**"
    ]
  }
}

